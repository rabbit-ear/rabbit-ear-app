{
  "useTabs": false,
  "singleQuote": false,
  "semi": true,
  "printWidth": 90,
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "plugins": [
    "prettier-plugin-svelte"
  ],
  "overrides": [{
    "files": "*.svelte",
    "options": {
      "parser": "svelte"
    }
  }]
}

